// Explicitly set Info.plist location
INFOPLIST_FILE = GoWay-iPadOS/SupportingFiles/Info.plist

// Prevent Xcode from generating Info.plist
GENERATE_INFOPLIST_FILE = NO

// Set code signing entitlements
CODE_SIGN_ENTITLEMENTS = GoWay-iPadOS/SupportingFiles/GoWay-iPadOS.entitlements

// Enable App Intents
ENABLE_APP_INTENTS = YES

// Basic settings
PRODUCT_BUNDLE_IDENTIFIER = com.goway.ipadapp
MARKETING_VERSION = 1.0
CURRENT_PROJECT_VERSION = 1
